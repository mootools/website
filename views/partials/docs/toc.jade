each section in toc
	- var visibility = section.text == file ? '' : 'small';
	ul.section		
		li(class="#{visibility}")
			if section.link
				a.top(href= section.link) #{section.text}

			if section.links && section.links.length
				ul
					each link in section.links
						li
							if (link.subTitle)
								h3: a(href= link.link) #{link.text}
							else 
								a(href= link.link) #{link.text}
